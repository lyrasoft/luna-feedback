{
    "extends": [
        "@windwalker-io/unicorn/tsconfig.js.json"
    ],
    "include": [
        "src/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ],
    "types": ["reflect-metadata"],
    "compilerOptions": {
        "allowJs": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noEmit": false,
        "strict": true,
    }
}
